{
  "name": "vscode-keys-counter",
	"displayName": "Keys Counter",
	"description": "Count how many changes in keys you have made in vscode editor.",
	"version": "1.0.0",
	"publisher": "HCl-10",
	"author": {
		"name": "Hyperhydrochloric Acid",
		"email": "HCl_10@outlook.com",
		"url": "https://github.com/HCl-10"
	},
	"license": "WTFPL",
	"engines": {
		"vscode": "^1.51.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"homepage": "https://github.com/HCl-10/vscode-keys-counter",
	"repository": {
		"type": "git",
		"url": "https://github.com/HCl-10/vscode-keys-counter.git"
	},
	"bugs": {
		"url": "https://github.com/HCl-10/vscode-keys-counter/issues"
	},
	"main": "./extension.js",
	"contributes": {
		"configuration": {
			"title": "Keys Counter",
			"properties": {
				"vscode-keys-counter.customString": {
					"type": "string",
					"default": "Keys count: {count}",
					"description": "The string showed in your keys counter status bar, use {count} to represent the counter."
				}
			}
		}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
        "@types/vscode": "^1.51.0",
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.0.0",
        "@types/node": "^12.11.7",
        "eslint": "^7.9.0",
        "glob": "^7.1.6",
        "mocha": "^8.1.3",
        "typescript": "^4.0.2",
		"vscode-test": "^1.4.0"
	}
}
